 #include <stdio.h>
 
 void swap(int *a, int *b) {
     int temp = *a;
     *a = *b;
     *b = temp;
 }
 
 int main(){
 	int n,i;
 	printf("enter the number of array: ");
 	scanf("%d",&n);
 	int array[n]; 
 	for(i=0;i<n;i++)
 	{
 		printf("enter the %d. number: ",i+1);
 		scanf("%d",&array[i]);
	 }
        
        int j;
        for(i=0; i<n; i++)
		{
            for(j=0; j<n; j++)
			{
                if(array[i] < array[j])
				{
                    swap(&array[i], &array[j]);
                }
            }
        }
        for(i=0; i<n; i++)
		{
            printf("%d ", array[i]);
        }
        return 0;
    }
